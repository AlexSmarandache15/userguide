              <entry><codeph>auto</codeph></entry>
            </row>
            <row>
              <entry>
                <codeph>'caption-side'</codeph>
              </entry>
              <entry>NONE</entry>
              <entry/>
            </row>
            <row>
              <entry>
                <codeph>'clear'</codeph>
              </entry>
