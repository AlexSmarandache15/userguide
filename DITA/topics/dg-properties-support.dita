              <entry/>
            </row>
            <row>
              <entry>
                <codeph>'border-collapse'</codeph>
              </entry>
              <entry>NONE</entry>
              <entry/>
            </row>
            <row>
              <entry>
                <codeph>'border-color'</codeph>
              </entry>
              <entry><codeph>&lt;color&gt; | inherit</codeph>
              </entry>
              <entry><codeph>transparent</codeph></entry>
            </row>
            <row>
              <entry>
                <codeph>'border-spacing'</codeph>
              </entry>
